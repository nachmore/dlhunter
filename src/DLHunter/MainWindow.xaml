<Window x:Class="DLHunter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Why Me?" Height="350" Width="600" MinWidth="600" MinHeight="350">
    <Grid>
        <Button Content="Select DL in Outlook" Click="btnFromOutlook_Click" Margin="10,13,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="48" Width="191" />
        <TextBlock Margin="239,32,0,0" VerticalAlignment="Top" HorizontalAlignment="Left">DL Alias@domain:</TextBlock>
        <Line Fill="Black" Stroke="Black" X1="0" X2="00" Y1="0" Y2="50" Margin="223,13,0,0" HorizontalAlignment="Left" VerticalAlignment="Top"/>
        <TextBox Name="txtAlias" HorizontalAlignment="Left" Height="23" Margin="340,28,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="115" KeyUp="txtAlias_KeyUp"/>
        <ListBox Name="lstMembers" Margin="0,86,0,0"></ListBox>
        <Button Content="Lookup By Alias" Click="btnFromAlias_Click" Margin="460,28,0,0" HorizontalAlignment="Left" VerticalAlignment="Top" Height="23" Width="107" />
        <TextBlock Name="txtTotalMembers" Margin="10,66,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"></TextBlock>
    </Grid>
</Window>
